page {
  background: #fff;
  height: 100%;
}

.content {
  padding-bottom: 20rpx;
  background: #fff;
}

.content.rp-flex-column {
  padding-bottom: 0;
  border-top: 1px solid #f1f1f1;
  box-sizing: border-box;
}

.card-wapper, .panle-power {
  position: relative;
  padding: 0 60rpx;
}

.gettit {
  font-size: 44rpx;
  color: #000;
  text-align: center;
  line-height: 100rpx;
}

.getcard {
  position: relative;
  width: 632rpx;
  height: 380rpx;
  margin: 0 auto 50rpx;
}

.getcard image {
  border-radius: 50%;
  width: 70rpx;
  height: 70rpx;
  margin: 0rpx 15rpx 0rpx 35rpx;
}

.getcard-content {
  padding: 38rpx 0 160rpx;
}

.getcard .icon-qr {
  display: inline-block;
  font-size: 46rpx;
  color: #fff;
}

.getcard-name text {
  display: block;
  color: #fefefe;
}

.getcard-name .f-s24 {
  margin-top: 10rpx;
}

.power-tit {
  display: flex;
  font-size: 26rpx;
  color: #222;
  align-items: center;
}

.power-tit .iconfont {
  font-size: 30rpx;
  margin-right: 8rpx;
}

.power-tip-list {
  margin-top: 52rpx;
  padding: 0 40rpx;
  font-size: 28rpx;
  line-height: 46rpx;
  color: #101010;
  vertical-align: middle;
}

.panle-recevie {
  margin-top: 48rpx;
}

.panle-isactive, .panle-recevie {
  padding: 40rpx 0 52rpx;
  text-align: center;
}

.panle-isactive navigator.btn, .panle-recevie .btn {
  display: inline-block;
  width: 406rpx;
  height: 78rpx;
  line-height: 78rpx;
  /* color: #3d925b; *//*text-align: center;*/
  border-radius: 4rpx;
  border: 1px solid transparent;
  font-size: 32rpx;
}

.panle-isactive text.tip, .panle-recevie text.tip {
  display: block;
  margin-top: 20rpx;
  font-size: 25rpx;
  color: #9e9e9e;
}

.panle-list {
  border-top: 1px solid #f0f0f0;
}

.panle-list .item {
  padding: 28rpx 32rpx;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #222;
  border-bottom: 1px solid #f0f0f0;
}

.panle-list .item.address {
  padding: 28rpx 0 28rpx 32rpx;
}

.panle-list .item .litte {
  display: block;
  margin-top: 10rpx;
  font-size: 23rpx;
  color: #9f9f9f;
}

.panle-list .item .icon-phone {
  display: block;
  width: 134rpx;
  height: 66rpx;
  text-align: center;
  font-size: 36rpx;
  line-height: 66rpx;
  /* background: url('https://pic.repaiapp.com/pic/7e/dc/2e/7edc2e1ce31a8b69879b4d4022b904a614238a2c.png') center no-repeat; *//* background-size: auto 36rpx; */
}

.panle-list .item .rp-flex-item.addr {
  position: relative;
}

.panle-list .item .rp-flex-item.addr::after {
  content: '';
  position: absolute;
  right: 0;
  top: 0;
  height: 100%;
  border-left: 1px solid #f0f0f0;
}

.panle-list .item .icon-right-more {
  color: #9f9f9f;
}

.panle-tab {
  align-items: center;
  padding: 0 60rpx;
  height: 108rpx;
  text-align: center;
}

.panle-tab .title {
  font-size: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  color: #222;
  margin-bottom: 26rpx;
}

.panle-tab .num ,.panle-tab .look{
  font-size: 36rpx;
  line-height: 36rpx;
}

.panle-tab .border-style {
  position: relative;
}

.panle-tab .border-style::after {
  content: '';
  position: absolute;
  right: 0;
  top: 0;
  height: 100%;
  border-left: 1px solid #f0f0f0;
}

.qr-number {
  align-items: center;
  padding: 0 40rpx 45rpx 40rpx;
}

.number-icont, .num-icont {
  display: inline-block;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  vertical-align: middle;
}

.num-icont {
  width: 22rpx;
  height: 38rpx;
}

.number-icont {
  width: 58rpx;
  height: 38rpx;
}

.qr-number .care-blank {
  padding: 20rpx 28rpx 0 28rpx;
}

.qr-number .icon-k-care {
  display: inline-block;
  font-size: 32rpx;
  color: #fff;
}

.qr-number .rp-flex-item {
  margin-top: 20rpx;
}

.logo-bottom {
  margin-top: 60rpx;
}

.icont-qr {
  width: 46rpx;
  height: 46rpx;
  background: url(https://pic.repaiapp.com/pic/f4/b0/08/f4b00876cacba351e99381da7cb5ff910f7b4b9f.png);
  background-size: 100% 100%;
  margin-right: 52rpx;
}


.disable-card{
  position: absolute;
  right: 0;
  top: 0;
  width: 150rpx;
  height: 149rpx;
  background: url('https://pic.repaiapp.com/pic/e3/62/f8/e362f8dbc6f1e367ac5e8c4544e8fa8ae1304cb6.png') center no-repeat;
  background-size: cover;
}