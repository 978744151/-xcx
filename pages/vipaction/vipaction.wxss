page {
    position: relative;
    background: #fefefe;
}

.vipaction-wapper {
    padding: 0 35rpx 110rpx 35rpx;
    border-top: 1px solid #f0f0f0;
}

.form-title {
    font-size: 28rpx;
    color: #9e9e9e;
    padding: 36rpx 0 14rpx;
    border-bottom: 1px solid #f0f0f0;
}

.form-item {
    display: -webkit-flex;
    display: flex;
    align-items: center;
    padding: 25rpx 0;
    border-bottom: 1px solid #f0f0f0;
    font-size: 30rpx;
}

.form-item .form-name {
    width: 115rpx;
    color: #222222;
}

.panle-agree {
    padding: 210rpx 35rpx 0 35rpx;
    text-align: center;
}

.panle-agree .agree-wapper {
    font-size: 28rpx;
    line-height: 48rpx;
}

.panle-agree .agree-wapper checkbox {
    transform: scale(.7);
}

.panle-agree .agree-wapper navigator {
    display: inline-block;
}

.form-btn {
    margin-top: 22rpx;
    height: 94rpx;
    line-height: 94rpx;
    font-size: 32rpx;
    background-color:rgb(239,62,64);
    color: #fff;
    border-radius: 4rpx;
}

.err-wapper {
    position: fixed;
    top: 0;
    width: 100%;
    height: 66rpx;
    line-height: 66rpx;
}

.err-tip {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background: #d65047;
    height: 66rpx;
    line-height: 66rpx;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    z-index: 2;
}

.tel-code {
    font-size: 28rpx;
    color: #a5a5a5;
}



.dialog-rule {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: 22;
    border-top: 1px solid #efefef;
    opacity: 0;
}

.dialog-rule .rp-flex-item {
    font-size: 28rpx;
    color: #000000;
    line-height: 180%;
}

.dialog-rule .rp-flex-item .wapper {
    padding: 0 34rpx 30rpx 34rpx;
}

.mar-b42 {
    margin-bottom: 42rpx;
}

.dialog-rule .title {
    height: 120rpx ;
    font-size: 42rpx;
    text-align: center;
    padding-top: 70rpx;
    font-weight: bold;
}

.dialog-rule .footer {
    height: 190rpx;
    text-align: center;
}

.dialog-rule .footer .close {
    display: inline-block;
    width: 68rpx;
    height: 68rpx;
    margin-top: 82rpx;
    background: url('https://pic.repaiapp.com/pic/6a/23/d3/6a23d35f040def0c689a299957b695d3230f0ac1.png') center no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
}
.handActive{
  font-size:24rpx;
  border: 1px solid #cccccc;
  line-height: 45rpx;
  text-align: center;
  margin-left: 10rpx;
  border-right: 10px;
  border: none;
  padding: 5rpx 10rpx;
}

.handActive.active{
  background: red;
  color:white;
}
/*选择器*/
.selector{ width: 100%; position: fixed; bottom: 0; left: 0; z-index: 999999}
.selector .inner_box{ position: relative; z-index: 999999}
.selector .header_box{ padding: 25rpx 0; overflow: hidden; box-sizing: border-box; background: #FBF9FE;}
.selector .header_box .item{ float: left; font-size: 34rpx; color: #586C94; box-sizing: border-box;}
.selector .header_box .l{ width: 25%; padding-left: 25rpx;}
.selector .header_box .c{ width: 50%; text-align: center}
.selector .header_box .r{ width: 25%; text-align: right; padding-right: 25rpx;}
.selector .selected{ height: 90rpx;}
.selector .picker_box{ width: 100%; height: 485rpx; background: #fff;}
.selector .picker_box picker-view-column{ text-align: center;}
.selector .picker_box picker-view-column view{ line-height: 90rpx;}
.selector .layer{ width: 100%; height: 100%; position: fixed; left: 0; top: 0; z-index: 10; background:rgba(0,0,0,0.5); }

.section picker{ display: inline-block; width: 50%;}
.section picker .picker{ color: #333}

.page_bottom{ background: #fff}
.page_bottom .sectionUpload{ padding: 20rpx; overflow: hidden; border-top: #C8C7CC solid 1.5rpx;}
.page_bottom .sectionUpload .item{ width: 160rpx; float: left;}
.page_bottom .sectionUpload .item image{ width: 150rpx; height: 150rpx;}
.page_bottom .sectionUpload .hint{ width: 300rpx; font-size: 30rpx; line-height: 150rpx; color: #828282}
