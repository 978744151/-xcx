page {
    background-color: #f5f5f5;
    color:#666;
}
.address-list{
  margin-bottom:120rpx;
}
.address-list .address {
    padding-left:30rpx;
    background-color: #fff;
    font-size: 27rpx;
    margin-bottom: 20rpx;
    box-sizing: border-box;

}

.address-list .address .address-radio {
    position: relative;
    /*display: block;*/
/*    padding: 0 20rpx 0 30rpx;
    float: left;*/
}

.address-list .address .address-info {
    display: inline-block;
    position: relative;
    padding-top: 15rpx;
    box-sizing: border-box;
}

.address-list .address .address-info .info-top {
    line-height: 50rpx;
    height: 50rpx;
}

.address-list .address .address-info .info-top .name {
    font-size: 30rpx;
    color: #000;
}

.address-list .address .address-info .info-top .phone {
    color: #353535;
    font-size: 28rpx;
    margin-left: 20rpx;
}

.address-list .address .address-info .info-bottom {
    position: relative;
     padding-bottom: 25rpx;
    /*margin-bottom: 10rpx;*/
}

.address-list .address .address-info .info-bottom .tag {
    display: block;
    height: 30rpx;
    line-height: 30rpx;
    background-color: #fe9900;
    font-size: 24rpx;
    color: #fff;
    padding: 4rpx 0;
    width: 64rpx;
    border-radius: 4rpx;
    margin-right: 20rpx;
    text-align: center;
}

.address-list .address .address-info .info-bottom .addr {
    /*height: 80rpx;*/
    color: #666;
}

.address-edit {
    display: block;
    background-repeat: no-repeat;
    background-size: cover;
    background-image: url('https://pic.repaiapp.com/static/jpg/20180904/14/1536041693963110250.jpg');
    width: 42rpx;
    height: 42rpx;
    margin-right: 30rpx;
    background-position: center;
}
.address-del {
    display: block;
    background-repeat: no-repeat;
    background-size: cover;
    background-image: url('https://pic.repaiapp.com/static/jpg/20180904/14/1536041519491115552.jpg');
    width: 42rpx;
    height: 42rpx;
    margin-right: 30rpx;
    background-position: center;
}
.newAddress {
    position: fixed;
    bottom: 0;
    width: 100%;
}

.newAddress .createAddress {
    font-size: 30rpx;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #fff;
    font-weight: bold;
    color: #f14949;
    text-align: center;
}

.newAddress .createAddress text {
    background-size: 30rpx 30rpx;
    background-repeat: no-repeat;
    background-image: url('data:image/png;base64,');
    padding-left: 40rpx;
    background-position:left center;
}

.noAddress {
    color: #b2b2b2;
    font-size: 28rpx;
    text-align: center;
    padding-top: 500rpx;
}

page {
    background: #f3f5f7;
}

.fr {
    float: right;
}

.pr20 {
    padding-right: 20rpx;
}

.cell-more {
    position: relative;
    padding-right: 30rpx;
    color: #bababa;
}

.cell-more:after {
    content: " ";
    height: 12rpx;
    width: 12rpx;
    border-width: 4rpx 4rpx 0 0;
    border-color: #C8C8CD;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    top: -4rpx;
    position: absolute;
    top: 50%;
    margin-top: -8rpx;
    right: 0rpx;
}


