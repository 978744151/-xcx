.imageGroup {
  width: 100%;
  height: 240rpx;
  position: relative;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f0f0f0;
  background: url('https://pic.repaiapp.com/static/png/20171220/10/1513738640239869949.png') no-repeat center;
  background-size: cover;
}

.imageGroup image {
  width: 100%;
  height: 100%;
}

.imageText {
  position: absolute;
  width: 100rpx;
  text-align: center;
  right: 0;
  bottom: 14rpx;
}

.imageText view {
  margin: 0 auto;
  width: 31rpx;
  height: 26rpx;
  background: url('https://pic.repaiapp.com/pic/a3/45/96/a34596385e60c3d6bc7c778962e019fafd00456e.png')no-repeat;
