}

.shopInfo {
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  border-bottom: 1px solid #f0f0f0;
}

.shopInfo .shopLogo {
  width: 162rpx;
  height: 162rpx;
  border: 1px solid #f0f0f0;
  overflow: hidden;
  margin-right: 22rpx;
}

.shopInfo .shopLogo image {
