page {
    background: #f6f6f6;
}

.shopimg {
    width: 100%;
    height: 306rpx;
}

.shopimg image {
    width: 750rpx;
    height: 306rpx;
    background: url('https://pic.repaiapp.com/pic/88/25/bc/8825bc5f24af3cde1d8e5d78047cac8814a3b951.png') center no-repeat;
    background-size: cover;
}

.shopnews {
    padding: 28rpx 32rpx;
    background: #fff;
}

.shop-tit {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
}

.shop-tit > text {
    font-size: 30rpx;
    color: #000000;
}

.shop-boon1,
.shop-boon2,
.shop-boon3,
.shop-boon4 {
    font-size: 22rpx;
    color: #ffffff;
    padding: 2rpx 8rpx;
    border-radius: 5rpx;
    margin-left: 15rpx;
}

.shop-boon1 {
    background: #fe675f;
}

.shop-boon2 {
    background: #5cb3e2;
}

.shop-boon3 {
    background: #f8c53d;
}

.shop-boon4 {
    background: #f07bad;
}

.shop-say {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
    margin-top: 20rpx;
}

.shop-price {
    font-size: 26rpx;
    color: #aaaaaa;
    margin-left: 20rpx;
}

.xx-hua {
    display: inline-block;
    width: 23rpx;
    height: 22rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
    margin-right: 5rpx;
}

.xx-hui {
    display: inline-block;
    width: 23rpx;
    height: 22rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
    margin-right: 5rpx;
}

.shopbox {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
    border-top: 1rpx solid #dedede;
    height: 104rpx;
    padding: 0 32rpx;
    background: #fff;
}

.shop-tel {
    width: 28rpx;
    height: 31rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.shop-text {
    flex: 1;
    margin-left: 20rpx;
    font-size: 28rpx;
    color: #7d7d7d;
}

.shop-right {
    width: 13rpx;
    height: 26rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.shop-area {
    width: 26rpx;
    height: 34rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.shop-time {
    width: 30rpx;
    height: 30rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.gonggao {
    margin-top: 20rpx;
    background: #fff;
}

.gonggao-tit {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
    font-size: 28rpx;
    color: #7d7d7d;
    height: 105rpx;
    padding: 0 32rpx;
    border-bottom: 1rpx solid #eaeaea;
}

.gonggao-tit text {
    display: block;
    width: 27rpx;
    height: 27rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
    margin-right: 20rpx;
}

.gonggao-text {
    padding: 30rpx 70rpx;
    font-size: 28rpx;
    color: #aaaaaa;
}
