.exchange-hint {
    margin-top: 20rpx;
    background: #fff;
}

.hint-tit {
    font-size: 28rpx;
    color: #9e9e9e;
    line-height: 80rpx;
    border-bottom: 1px solid #f0f0f0;
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
}

.hint-tit text {
    display: inline-block;
    width: 4rpx;
    height: 24rpx;
    background: #9e9e9e;
    margin: 0 15rpx 0 25rpx;
}

.hint-text {
    padding: 20rpx 40rpx 30rpx;
    font-size: 28rpx;
    color: #101010;
    line-height: 50rpx;
}

.hint-text text {
    display: block;
}

.exchange-other {
    padding:15rpx 20rpx;
    display: flex;
    align-items: center;
    color: #9e9e9e;
    font-size: 28rpx;
    background: #fff;
    border-bottom: 1px solid #f0f0f0;
}

.exchange-other .rp-flex-item.left {
    text-decoration: line-through;
}

.exchange-other .rp-flex-item.right {
    text-align: right;
}

.exchange-other .rp-flex-item.center {
    text-align: center;
}

.exchang-money {
    background: #fff;
    padding: 0 25rpx;
    color: #f14949;
    font-size: 0;
    line-height: normal;
}

.exchang-money .strong {
    font-size: 37rpx;
    font-weight: 500;
}
