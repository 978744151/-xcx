page {
    background: #f6f6f6;
    height: 100%;
    width: 100%;
}

.content {
    position: relative;
    z-index: 1001;
    padding-bottom: 100rpx;
}

.swiper,
.banner {
    width: 100%;
    height: 470rpx;
}

.banner image {
    width: 100%;
    height: 470rpx;
    background: url('https://pic.repaiapp.com/pic/1b/fb/ac/1bfbac487e8aca2f513361287609fef25fcf9c19.png') center no-repeat;
    background-size: cover;
}

.goodsinfor {
    position: relative;
    /*background-color: #fff;*/
}

.jifen {
    position: absolute;
    right: 10px;
    color: #ff7b11;
    font-size: 32rpx;
}

.title {
    position: relative;
    padding: 17rpx 25rpx 10rpx 25rpx;
    font-size: 0;
    background-color: #fff;
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
}

.title .xuni,.xushi {
  display:inline-block;
  width:56rpx;
  height:32rpx;
  color:#fff;
  margin-right:15rpx;
  white-space: nowrap;
}

.titleCon {
    font-size: 32rpx;
    color: #101010;
    flex: 1;
}

.name {
    position: relative;
    padding: 30rpx 20rpx;
    padding-top: 0;
    background-color: #fff;
    /*font-size: 0*/
}

.name text {
    font-size: 14px;
    color: #9e9e9e;
    text-decoration: line-through;
}

.name text:first-child {
    color: #ff7b11;
    text-decoration: none;
    margin-right: 10px;
}

.name text:last-child {
    text-decoration: none;
    position: absolute;
    right: 10px;
}

.validity {
    position: relative;
    padding: 15rpx 25rpx 30rpx 25rpx;
    font-size: 28rpx;
    color: #9e9e9e;
    background-color: #fff;
    line-height: 28rpx;
}

.bordernone {
    border: none;
    padding-top: 0;
}

.address {
    position: relative;
    margin-top: 10px;
    border-top: 1rpx #e8e8e8 solid;
    background-color: #fff;
    padding: 20rpx;
    font-size: 14px;
    color: #9e9e9e;
}

.address view {
    width: 2px;
    height: 22rpx;
    display: inline-block;
    background-color: #9e9e9e;
}

.addressList {
    position: relative;
    padding: 10px;
    background-color: #f9f5f5;
    border-bottom: 1px #e8e8e8 solid;
    font-size: 14px;
    color: #9e9e9e;
    z-index: 1000;
}

.addressList text {
    color: #101010;
}

.activityRules {
    position: relative;
    background-color: #fff;
    padding: 10px;
    padding-left: 15.5px;
    font-size: 14px;
    color: #101010;
    border-top: 1rpx #e8e8e8 solid;
    border-bottom: 1rpx #e8e8e8 solid;
}

.activityRules view {
    margin-bottom: 10px;
}

.image-text {
    position: relative;
    background-color: #fff;
    height: 380rpx;
    width: 100%;
}

.image-text image {
    width: 100%;
    height: 100%;
}

.footer {
    position: fixed;
    bottom: 0;
    height: 103rpx;
    line-height: 103rpx;
    text-align: center;
    background-color: #dd2f3b;
    color: #fff;
    font-size: 32rpx;
    width: 100%;
    z-index: 1002;
}

.addressCon {
    position: fixed;
    top: 0;
    height: 100%;
    width: 100%;
    background-color: #f6f6f6;
    z-index: 1002;
    border-top: 1rpx #e8e8e8 solid;
    transition: all 0.5s ease;
}


