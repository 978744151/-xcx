.pickerTime {
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
  display: flex;
  align-items: flex-end;
  z-index: 999;
}

.pickerTime_content {
  width: 100%;
  height: 500rpx;
  background: #fff;
  transition: all 0.2s linear;
  position: fixed;
  bottom: 0;
  z-index: 1000;
}

.pickerTime_show {
  transform: translateY(0);
}

.pickerTime_hide {
  transform: translateY(100%);
}

.pickerTime_title {
  line-height: 75rpx;
  text-align: center;
  background: #f2f2f2;
  font-size: 32rpx;
}

.pickerTime_select {
  height: 425rpx;
  display: flex;
}

.pickerTime_select .pickerTime_left {
  height: 100%;
  background: #f2f2f2;
  overflow-y: auto;
  width: 210rpx;
  flex: none;
}

.pickerTime_left .pickerTime_left_item {
  line-height: 85rpx;
  text-align: center;
  font-size: 28rpx;
}

.pickerTime_left .pickerTime_left_item.active {
  color: #969696;
  background: #fff;
}

.pickerTime_select .pickerTime_right {
  height: 100%;
  overflow-y: auto;
  flex: auto;
  margin-left: 30rpx;
}

.pickerTime_right .pickerTime_right_item {
  line-height: 85rpx;
  font-size: 28rpx;
  border-bottom: 1px solid #f7f6f7;
