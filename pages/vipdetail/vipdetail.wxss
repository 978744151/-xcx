page {
    background: #fff;
}

.panle-detail {
    border-top: 1rpx solid #ebebeb;
    padding: 34rpx 30rpx;
}

.panle-detail .box {
    display: -webkit-flex;
    display: flex;
    font-size: 28rpx;
    line-height: 50rpx;
    margin-top: 20rpx;
}

.panle-detail .box:frist-of-type {
    margin-top: 0;
}

.panle-detail .left-span {
    display: block;
    width: 135rpx;
    color: #939393;
    white-space: nowrap;
}

.panle-detail .right-span {
    flex: 1;
    padding-right: 35rpx;
}

.listitem {
    color: #000000;
    margin-bottom: 10rpx;
}

.panle-detail .tag {
    font-size: 24rpx;
    color: #a9a7a7;
    padding: 8rpx;
    background: #eeedfc;
    margin-right: 10rpx;
}

.color-679{
    color:#679e64;
}